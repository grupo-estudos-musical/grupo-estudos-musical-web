namespace FastReport
{
  public class ReportScript
  {
    public int numeroLinha = 1;
    
    

    private void Shape6_BeforePrint(object sender, EventArgs e)
    {
      if(numeroLinha % 2 == 0)
      {
         retanguloAvaliacao.FillColor = Color.White;
      }else
      {
         retanguloAvaliacao.FillColor = Color.LightBlue;
      }
      numeroLinha += 1;  
    }

   

    private void media_AfterData(object sender, EventArgs e)
    {
      double mediaAluno = Convert.ToDouble(media.Value);
      if(mediaAluno &lt; 6)
      {
        media.TextColor = Color.Red;
      }else
      {
        media.TextColor = Color.Blue;
      }
    }

    private void situacaoMatricula_AfterData(object sender, EventArgs e)
    {
      if(situacaoMatricula.Text == &quot;Retido&quot;)
      {
         situacaoMatricula.TextColor = Color.Red;
      }else
      {
         situacaoMatricula.TextColor = Color.Blue;
      }  
    }

    private void GroupHeader1_AfterData(object sender, EventArgs e)
    {
      numeroLinha = 1;
    }
  }
}                                             
